{
  "name": "roeurnz/gss-bot",
  "description": "A PHP-based Telegram bot GSS that interacts with the Telegram API to provide automated responses and functionalities.",
  "type": "library",
  "require": {
    "php": "^8.2|^8.3.10",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "guzzlehttp/guzzle": "^7.9.2",
    "shuchkin/simplexlsxgen": "^1.3.10",
    "vlucas/phpdotenv": "^5.6.1",
    "symfony/dotenv": "^7.1.3"
  },

  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "App\\GssBot\\": "app/"
    }
  },
  "authors": [
    {
      "name": "ROEURNZ",
      "email": "roeurnkaki@gmail.com",
      "homepage": "https://github.com/ROEURNZ",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
