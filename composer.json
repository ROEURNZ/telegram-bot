{
    "name": "app/telegram-bot",
    "description": "A PHP-based Telegram bot that interacts with the Telegram API to provide automated responses and functionalities.",
    "type": "library",
    "require": {
        "php": "^8.2|^8.3.10",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.9",
        "php-http/discovery": "^1.19.4",
        "vlucas/phpdotenv": "^5.6.1",
        "monolog/monolog": "^3.7.0",
        "respect/validation": "^2.3.7",
        "symfony/cache": "^7.1.4",
        "symfony/dotenv": "^7.1.3",
        "guzzlehttp/psr7": "^2.7.0",
        "thiagoalessio/tesseract_ocr": "^2.13",
        "symfony/intl": "^7.1.1",
        "voku/portable-ascii": "^2.0.1",
        "tarfin-labs/zbar-php": "^1.7"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.4.0",
        "phpunit/phpunit": "^11.3.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\TelegramBot\\": "app/"
        }
    },
    "authors": [
        {
            "name": "ROEURNZ",
            "email": "roeurnkaki@gmail.com",
            "homepage": "https://github.com/ROEURNZ",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    } 
}
